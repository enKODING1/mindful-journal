{
    "name": "mindful-journal",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
        "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,css}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@next/third-parties": "^15.5.6",
        "@supabase/auth-helpers-nextjs": "^0.10.0",
        "@supabase/ssr": "^0.7.0",
        "@supabase/supabase-js": "^2.57.2",
        "lucide-react": "^0.543.0",
        "next": "15.5.2",
        "react": "19.1.0",
        "react-day-picker": "^9.9.0",
        "react-dom": "19.1.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@typescript-eslint/eslint-plugin": "^8.42.0",
        "@typescript-eslint/parser": "^8.42.0",
        "daisyui": "^5.1.7",
        "eslint": "^9.35.0",
        "eslint-config-next": "15.5.2",
        "eslint-config-prettier": "^10.1.8",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.3",
        "prettier": "^3.6.2",
        "tailwindcss": "^4",
        "typescript": "^5"
    }
}
